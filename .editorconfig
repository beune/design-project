# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# Matches multiple files with brace expansion notation
# Set default charset
[*]
end_of_line = lf
charset = utf-8
indent_style = space
insert_final_newline = true

[*.{js,vue}]
indent_size = 2

[*.py]
indent_size = 4
